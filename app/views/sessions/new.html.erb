

<div class="row">
  <div class="col s12">
    <h3 class="center-align">Log in</h3>
  </div>
</div>



  <%= form_with scope: :session, url: login_path, local: true do |f|%>
  <div class="row">
   

    <div class="input-field col s12" >
      <%= f.label :email %>
      <%= f.email_field :email %>
    </div>
  </div>
  
  <div class="row">
    <div class="input-field col s12">
      <%= f.label :password %>
      <%= f.password_field :password %>
    </div>

  </div>
    <%= f.submit "Log in", class: "waves-effect waves-light btn"%>
  <% end %>
