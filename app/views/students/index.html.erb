<div class="row">
  <div class="col s12">
    <h3 class="center-align">STUDENTS</h3>
  </div>
</div>

<% @students.each do |student| %>
 
<div class="row">
  <div class="col s6 m6">
    <div class="card blue-grey darken-1">
      <div class="card-content white-text">
        <span class="card-title"><%= link_to student.name, student_path(student) %></span>
        <p><%= student.email %></p>
      </div>
      <div class="card-action">
        <a href="#">Enroll</a>
        <a href="#">More information</a>
      </div>
    </div>
  </div>
<% end %>