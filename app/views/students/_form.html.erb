

        <%= form_for @student do |f|%>
          <%= f.label :name %>
          <%= f.text_field :name %>

          <%= f.label :email %>
          <%= f.email_field :email %>

          <%= f.label :password %>
          <%= f.password_field :password %>

          <%= f.submit class: "waves-effect waves-light btn"%>
        <% end %>
